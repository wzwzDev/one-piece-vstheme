  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6a9955"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#dc143c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#ff4500"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "scope": ["number", "constant.numeric"],
      "settings": {
        "foreground": "#4a90e2"
      }
    },
    {
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#00bcd4"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#32cd32"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#ffd700",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#dc143c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ff4500",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4a90e2",
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#4ec9b0",
    "class": "#4ec9b0",
    "enum": "#4ec9b0",
    "interface": "#4ec9b0",
    "struct": "#4ec9b0",
    "typeParameter": "#4ec9b0",
    "type": "#4ec9b0",
    "parameter": "#dcdcaa",
    "variable": "#9cdcfe",
    "property": "#9cdcfe",
    "enumMember": "#4fc1ff",
    "event": "#dcdcaa",
    "function": "#32cd32",
    "method": "#32cd32",
    "macro": "#ff4500",
    "keyword": "#dc143c",
    "modifier": "#dc143c",
    "comment": "#6a9955",
    "string": "#ffd700",
    "number": "#4a90e2",
    "regexp": "#ff69b4",
    "operator": "#ff4500"
